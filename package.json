{
  "name": "starter-express",
  "version": "0.1.0",
  "description": "A ready to use express web app for fast prototyping, or debugging.",
  "main": "main.js",
  "scripts": {
    "lint": "eslint server/**",
    "start": "cross-env NODE_PATH=./ node main.js",
    "start:debug": "cross-env NODE_PATH=./ node --inspect-brk main.js",
    "test": "npm run lint"
  },
  "keywords": [
    "express",
    "web-app"
  ],
  "author": "Bruno Scopelliti",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.18.2",
    "chalk": "2.4.1",
    "cookie-parser": "1.4.3",
    "cross-env": "5.1.5",
    "errorhandler": "1.5.0",
    "express": "4.16.3",
    "hbs": "4.0.1",
    "morgan": "1.9.0",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-bruno": "1.3.0",
    "pre-commit": "1.2.2"
  }
}
