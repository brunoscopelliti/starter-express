{
  "name": "starter-express",
  "version": "0.1.0",
  "description": "A ready to use express web app for fast prototyping, or debugging.",
  "main": "main.js",
  "scripts": {
    "lint": "eslint 'server/**'",
    "start": "cross-env NODE_PATH=./ node main.js",
    "start:debug": "cross-env NODE_PATH=./ node --inspect-brk main.js",
    "test": "npm run lint"
  },
  "keywords": [
    "express",
    "web-app"
  ],
  "author": "Bruno Scopelliti",
  "license": "MIT",
  "dependencies": {
    "@jscore/envar": "1.0.0",
    "body-parser": "1.19.0",
    "chalk": "2.4.2",
    "cookie-parser": "1.4.4",
    "cross-env": "5.2.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "hbs": "4.0.4",
    "morgan": "1.9.1",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "eslint": "6.0.1",
    "eslint-config-bruno": "2.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "pre-commit": "1.2.2"
  }
}
